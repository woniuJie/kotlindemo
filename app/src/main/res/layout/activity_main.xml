<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="协程网络请求"
        android:onClick="onCorClick"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="双击点赞"
        android:onClick="onDianZanClick"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="协程练习"
        android:onClick="onXieCheng"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="毛玻璃"
        android:onClick="onMaoBoLi"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="事件处理"
        android:onClick="onEventClick"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="viewpager"
        android:onClick="onViewPager"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AsyncLayoutInflate"
        android:onClick="onAsync"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="内存优化"
        android:onClick="onOOMEvent"
        />


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="bitmap"
        android:onClick="onBitmap"
        />


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="倒计时"
        android:onClick="onCustTimer"
        />


</LinearLayout>